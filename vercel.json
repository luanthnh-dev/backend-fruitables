{
  "builds": [
    {
      "src": "fruitables/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "250mb", "runtime": "python3.9" }
    },
    {
      "src": "build_files.sh",
      "use": "@vercel/static-build",
      "config": { "distDir": "staticfiles_build" }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "fruitables/wsgi.py"
    }
  ]
}
